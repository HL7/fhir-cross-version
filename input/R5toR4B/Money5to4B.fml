/// url = "http://hl7.org/fhir/StructureMap/Money5to4B"
/// name = "Money5to4B"
/// title = "Money Transforms: R5 to R4B"
/// status = "active"

uses "http://hl7.org/fhir/5.0/Money" alias MoneyR5 as source
uses "http://hl7.org/fhir/4.0/Money" alias MoneyR4B as target

imports "http://hl7.org/fhir/StructureMap/*5to4B"

group Money(source src : MoneyR5, target tgt : MoneyR4B) extends Element <<type+>> {
  src.value -> tgt.value;
  src.currency -> tgt.currency;
}

