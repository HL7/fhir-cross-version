/// url = "http://hl7.org/fhir/StructureMap/Reference3to2"
/// name = "Reference3to2"
/// title = "Reference Transforms: R3 to R2"
/// status = "active"

uses "http://hl7.org/fhir/3.0/Reference" alias Reference as source
uses "http://hl7.org/fhir/1.0/Reference" alias ReferenceR2 as target

imports "http://hl7.org/fhir/StructureMap/primitives3to2"
imports "http://hl7.org/fhir/StructureMap/Element3to2"

group Reference(source src : Reference, target tgt : ReferenceR2) extends Element <<type+>> {
  src.reference -> tgt.reference "Reference-reference";
  src.display -> tgt.display "Reference-display";
}

