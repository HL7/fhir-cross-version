/// url = "http://hl7.org/fhir/StructureMap/Contributor5to4B"
/// name = "Contributor5to4B"
/// title = "Contributor Transforms: R5 to R4B"
/// status = "active"

uses "http://hl7.org/fhir/5.0/Contributor" alias ContributorR5 as source
uses "http://hl7.org/fhir/4.0/Contributor" alias ContributorR4B as target

imports "http://hl7.org/fhir/StructureMap/*5to4B"

group Contributor(source src : ContributorR5, target tgt : ContributorR4B) extends Element <<type+>> {
  src.type as v -> tgt.type = translate(v, 'http://hl7.org/fhir/ConceptMap/Contributor.type-R5toR4', 'code');
  src.name -> tgt.name;
  src.contact -> tgt.contact;
}

