group ResearchStudyComparisonGroup(source src, target tgt) extends BackboneElement {
  src.name -> tgt.name;
  src.type -> tgt.type;
  src.description -> tgt.description;
}

group ResearchStudyObjective(source src, target tgt) extends BackboneElement {
  src.name -> tgt.name;
  src.type -> tgt.type;
}

