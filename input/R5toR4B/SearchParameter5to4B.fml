/// url = "http://hl7.org/fhir/uv/xver/StructureMap/SearchParameter5to4b"
/// name = "SearchParameter5to4b"
/// title = "SearchParameter Transforms: R5 to R4B"
/// status = "active"

uses "http://hl7.org/fhir/5.0/SearchParameter" alias SearchParameterR5 as source
uses "http://hl7.org/fhir/4.3/SearchParameter" alias SearchParameterR4B as target

imports "http://hl7.org/fhir/uv/xver/StructureMap/*5to4b"

group SearchParameter(source src : SearchParameterR5, target tgt : SearchParameterR4B) extends DomainResource <<type+>> {
  src.url -> tgt.url;
  src.version -> tgt.version;
  src.name -> tgt.name;
  src.derivedFrom -> tgt.derivedFrom;
  src.status as v -> tgt.status = translate(v, 'http://hl7.org/fhir/uv/xver/ConceptMap/SearchParameter.status-5to4b', 'code');
  src.experimental -> tgt.experimental;
  src.date -> tgt.date;
  src.publisher -> tgt.publisher;
  src.contact -> tgt.contact;
  src.description -> tgt.description;
  src.useContext -> tgt.useContext;
  src.jurisdiction -> tgt.jurisdiction;
  src.purpose -> tgt.purpose;
  src.code -> tgt.code;
  src.base -> tgt.base;
  src.type as v -> tgt.type = translate(v, 'http://hl7.org/fhir/uv/xver/ConceptMap/SearchParameter.type-5to4b', 'code');
  src.expression -> tgt.expression;
  src.processingMode as v -> tgt.xpathUsage = translate(v, 'http://hl7.org/fhir/uv/xver/ConceptMap/SearchParameter.processingMode-SearchParameter.xpathUsage-5to4b', 'code');
  src.target -> tgt.target;
  src.multipleOr -> tgt.multipleOr;
  src.multipleAnd -> tgt.multipleAnd;
  src.comparator as v -> tgt.comparator = translate(v, 'http://hl7.org/fhir/uv/xver/ConceptMap/SearchParameter.comparator-5to4b', 'code');
  src.modifier as v -> tgt.modifier = translate(v, 'http://hl7.org/fhir/uv/xver/ConceptMap/SearchParameter.modifier-5to4b', 'code');
  src.chain -> tgt.chain;
  src.component as s -> tgt.component as t then SearchParameterComponent(s, t);
}

group SearchParameterComponent(source src, target tgt) extends BackboneElement {
  src.definition -> tgt.definition;
  src.expression -> tgt.expression;
}

