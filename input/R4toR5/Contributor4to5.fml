/// url = "http://hl7.org/fhir/StructureMap/Contributor4to5"
/// name = "Contributor4to5"
/// title = "Contributor Transforms: R4 to R5"
/// status = "active"

uses "http://hl7.org/fhir/4.0/Contributor" alias ContributorR4 as source
uses "http://hl7.org/fhir/5.0/Contributor" alias ContributorR5 as target

imports "http://hl7.org/fhir/StructureMap/*4to5"

group Contributor(source src : ContributorR4, target tgt : ContributorR5) extends Element <<type+>> {
  src.type as v -> tgt.type = translate(v, 'http://hl7.org/fhir/ConceptMap/Contributor.type-R4toR5', 'code');
  src.name -> tgt.name;
  src.contact -> tgt.contact;
}

