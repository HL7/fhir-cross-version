/// url = "http://hl7.org/fhir/StructureMap/OperationOutcome4Bto5"
/// name = "OperationOutcome4Bto5"
/// title = "OperationOutcome Transforms: R4B to R5"
/// status = "active"

uses "http://hl7.org/fhir/4.3/OperationOutcome" alias OperationOutcomeR4B as source
uses "http://hl7.org/fhir/5.0/OperationOutcome" alias OperationOutcomeR5 as target

imports "http://hl7.org/fhir/StructureMap/*4Bto5"

group OperationOutcome(source src : OperationOutcomeR4B, target tgt : OperationOutcomeR5) extends DomainResource <<type+>> {
  src.issue as s -> tgt.issue as t then OperationOutcomeIssue(s, t);
}

group OperationOutcomeIssue(source src, target tgt) extends BackboneElement {
  src.severity as v -> tgt.severity = translate(v, 'http://hl7.org/fhir/ConceptMap/OperationOutcomeB.issue.severity-R4BtoR5', 'code');
  src.code as v -> tgt.code = translate(v, 'http://hl7.org/fhir/ConceptMap/OperationOutcomeB.issue.code-R4BtoR5', 'code');
  src.details -> tgt.details;
  src.diagnostics -> tgt.diagnostics;
  src.location -> tgt.location;
  src.expression -> tgt.expression;
}

