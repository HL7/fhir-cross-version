/// url = "http://hl7.org/fhir/StructureMap/Organization5to4B"
/// name = "Organization5to4B"
/// title = "Organization Transforms: R5 to R4B"
/// status = "active"

uses "http://hl7.org/fhir/5.0/Organization" alias OrganizationR5 as source
uses "http://hl7.org/fhir/4.0/Organization" alias OrganizationR4B as target

imports "http://hl7.org/fhir/StructureMap/*5to4B"

group Organization(source src : OrganizationR5, target tgt : OrganizationR4B) extends DomainResource <<type+>> {
  src.identifier -> tgt.identifier;
  src.active -> tgt.active;
  src.type -> tgt.type;
  src.name -> tgt.name;
  src.alias -> tgt.alias;
  src.contact as s -> tgt.contact as t then {
    s.purpose -> t.purpose;
    s.name -> t.name;
    s.telecom -> t.telecom;
    s.address -> t.address;
  };
  src.partOf -> tgt.partOf;
  src.endpoint -> tgt.endpoint;
}

