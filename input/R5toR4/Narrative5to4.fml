/// url = "http://hl7.org/fhir/StructureMap/Narrative5to4"
/// name = "Narrative5to4"
/// title = "Narrative Transforms: R5 to R4"
/// status = "active"

uses "http://hl7.org/fhir/5.0/Narrative" alias NarrativeR5 as source
uses "http://hl7.org/fhir/4.0/Narrative" alias NarrativeR4 as target

imports "http://hl7.org/fhir/StructureMap/*5to4"

group Narrative(source src : NarrativeR5, target tgt : NarrativeR4) extends Element <<type+>> {
  src.status as v -> tgt.status = translate(v, 'http://hl7.org/fhir/ConceptMap/Narrative.status-R5toR4', 'code');
  src.div -> tgt.div;
}

