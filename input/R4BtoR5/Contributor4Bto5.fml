/// url = "http://hl7.org/fhir/StructureMap/Contributor4Bto5"
/// name = "Contributor4Bto5"
/// title = "Contributor Transforms: R4B to R5"
/// status = "active"

uses "http://hl7.org/fhir/4.3/Contributor" alias ContributorR4B as source
uses "http://hl7.org/fhir/5.0/Contributor" alias ContributorR5 as target

imports "http://hl7.org/fhir/StructureMap/*4Bto5"

group Contributor(source src : ContributorR4B, target tgt : ContributorR5) extends Element <<type+>> {
  src.type as v -> tgt.type = translate(v, 'http://hl7.org/fhir/ConceptMap/ContributorB.type-R4BtoR5', 'code');
  src.name -> tgt.name;
  src.contact -> tgt.contact;
}

