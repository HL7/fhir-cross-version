/// url = "http://hl7.org/fhir/uv/xver/StructureMap/Period5to4"
/// name = "Period5to4"
/// title = "Period Transforms: R5 to R4"
/// status = "active"

uses "http://hl7.org/fhir/5.0/Period" alias PeriodR5 as source
uses "http://hl7.org/fhir/4.0/Period" alias PeriodR4 as target

imports "http://hl7.org/fhir/uv/xver/StructureMap/*5to4"

group Period(source src : PeriodR5, target tgt : PeriodR4) extends Element <<type+>> {
  src.start -> tgt.start;
  src.end -> tgt.end;
}

