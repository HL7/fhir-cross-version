/// url = "http://hl7.org/fhir/StructureMap/Narrative3to2"
/// name = "Narrative3to2"
/// title = "Narrative Transforms: R3 to R2"
/// status = "active"

uses "http://hl7.org/fhir/3.0/Narrative" alias Narrative as source
uses "http://hl7.org/fhir/1.0/Narrative" alias NarrativeR2 as target

imports "http://hl7.org/fhir/StructureMap/primitives3to2"
imports "http://hl7.org/fhir/StructureMap/Element3to2"

group Narrative(source src : Narrative, target tgt : NarrativeR2) extends Element <<type+>> {
  src.status -> tgt.status "Narrative-status";
  src.div -> tgt.div "Narrative-div";
}

