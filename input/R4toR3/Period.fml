/// url = "http://hl7.org/fhir/uv/xver/StructureMap/Period4to3"
/// name = "Period4to3"
/// title = "Period Transforms: R4 to R3"
/// status = "active"

uses "http://hl7.org/fhir/4.0/Period" alias Period as source
uses "http://hl7.org/fhir/3.0/Period" alias PeriodR3 as target

imports "http://hl7.org/fhir/uv/xver/StructureMap/*4to3"

group Period(source src : Period, target tgt : PeriodR3) extends Element <<type+>> {
  src.start -> tgt.start;
  src.end -> tgt.end;
}

