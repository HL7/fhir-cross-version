/// url = "http://hl7.org/fhir/StructureMap/Period4to5"
/// name = "Period4to5"
/// title = "Period Transforms: R4 to R5"
/// status = "active"

uses "http://hl7.org/fhir/4.0/Period" alias PeriodR4 as source
uses "http://hl7.org/fhir/5.0/Period" alias PeriodR5 as target

imports "http://hl7.org/fhir/StructureMap/*4to5"

group Period(source src : PeriodR4, target tgt : PeriodR5) extends Element <<type+>> {
  src.start -> tgt.start;
  src.end -> tgt.end;
}

